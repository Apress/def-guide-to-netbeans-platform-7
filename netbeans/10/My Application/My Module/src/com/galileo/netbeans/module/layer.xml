<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.2//EN" "http://www.netbeans.org/dtds/filesystem-1_2.dtd">
<filesystem>
    <folder name="Actions">
        <folder name="Window">
            <file name="com-galileo-netbeans-module-MyTopComponent.instance">
                <attr
                    methodvalue="org.openide.windows.TopComponent.openAction" name="instanceCreate"/>
                <attr name="component" newvalue="com.galileo.netbeans.module.MyTopComponent"/>
                <attr name="preferredID" stringvalue="MyTopComponent"/>
                <attr
                    bundlevalue="com.galileo.netbeans.module.Bundle#CTL_MyAction" name="displayName"/>
                <attr name="iconBase" stringvalue="com/galileo/netbeans/module/icon.png"/>
            </file>
        </folder>
    </folder>
    <folder name="Menu">
        <folder name="Window">
            <file name="com-galileo-netbeans-module-MyTopComponent.shadow">
                <attr name="originalFile" stringvalue="Actions/Window/com-galileo-netbeans-module-MyTopComponent.instance"/>
            </file>
        </folder>
    </folder>
    <folder name="Windows2">
        <folder name="Components">
            <file name="MyTopComponent.settings" url="MyTopComponentSettings.xml"/>
        </folder>
        <folder name="Modes">
            <folder name="editor">
                <file name="MyTopComponent.wstcref" url="MyTopComponentWstcref.xml"/>
            </folder>
        </folder>
    </folder>
</filesystem>
